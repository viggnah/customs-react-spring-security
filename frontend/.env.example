# WSO2 Identity Server Configuration
# Copy this file to .env and update with your actual values

# WSO2 Identity Server base URL
REACT_APP_WSO2_BASE_URL=https://localhost:9443

# OAuth2 Client ID (configured in WSO2 IS)
REACT_APP_WSO2_CLIENT_ID=YOUR_CLIENT_ID_HERE
REACT_APP_WSO2_CLIENT_SECRET=YOUR_CLIENT_SECRET_HERE

# Redirect URLs (must be registered in WSO2 IS)
REACT_APP_WSO2_SIGNIN_REDIRECT_URL=http://localhost:3000/auth/callback
REACT_APP_WSO2_SIGNOUT_REDIRECT_URL=http://localhost:3000/login

# Backend API URL
REACT_APP_API_BASE_URL=http://localhost:8080/api
